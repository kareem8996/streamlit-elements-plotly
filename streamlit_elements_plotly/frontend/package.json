{
  "name": "streamlit-elements",
  "private": true,
  "scripts": {
    "clean": "rimraf build",
    "dev": "npm run build-public && next dev -H 127.0.0.1 -p 3001",
    "build": "npm run build-public && next build && next export -o build",
    "start": "next start",
    "lint": "next lint",
    "build-public": "npm run copy-monaco && npm run del-source-maps",
    "del-source-maps": "powershell -Command \"Get-ChildItem -Recurse -Filter *.js -Path public | ForEach-Object { (Get-Content $_.FullName) -replace 'sourceMappingURL.*', '' | Set-Content $_.FullName }\"",
    "copy-monaco": "if not exist public\\monaco mkdir public\\monaco && rmdir /s /q public\\monaco\\vs && xcopy node_modules\\monaco-editor\\min\\vs public\\monaco\\vs /E /I"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@monaco-editor/react": "^4.3.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.73",
    "@mui/material": "^5.5.1",
    "@mui/x-data-grid": "^5.6.1",
    "@tinymce/tinymce-react": "^3.13.1",
    "dequal": "^2.0.2",
    "monaco-editor": "^0.33.0",
    "mousetrap": "^1.6.5",
    "next": "12.1.0",
    "plotly.js-dist-min": "^2.35.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-grid-layout": "^1.3.4",
    "react-player": "^2.10.0",
    "react-plotly.js": "^2.5.1",
    "react-swipeable-views": "^0.14.0",
    "resize-observer-polyfill": "^1.5.1",
    "streamlit-component-lib": "^1.4.0"
  },
  "devDependencies": {
    "@types/json5": "^0.0.30",
    "@types/mousetrap": "^1.6.9",
    "@types/node": "17.0.21",
    "@types/plotly.js": "^2.35.1",
    "@types/react": "17.0.41",
    "@types/react-grid-layout": "^1.3.2",
    "@types/react-swipeable-views": "^0.13.1",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "rimraf": "^6.0.1",
    "typescript": "4.6.2"
  }
}
